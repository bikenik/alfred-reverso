{
	"name": "alfred-reverso",
	"version": "1.0.4",
	"description": "Quick look from ContextReverso.net and create flash cards to Anki",
	"license": "MIT",
	"repository": "bikenik/alfred-reverso",
	"author": {
		"name": "bikeNik",
		"email": "bikenik@icloud.com",
		"url": "http://bikenik.org"
	},
	"engines": {
		"node": ">=7.6"
	},
	"scripts": {
