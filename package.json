		"google-translate-api": "github:bikenik/google-translate-api",
		"got": "^9.2.2",
		"jsonfile": "^5.0.0",
		"languagedetect": "^1.1.1",
		"md5": "^2.2.1",
		"p-map": "^2.0.0",
		"promise": "^8.0.2",
		"request": "^2.88.0",
		"request-promise": "^4.2.2",
		"run-applescript": "^3.2.0",
		"stream-to-promise": "^2.2.0",
		"xmlhttprequest": "^1.8.0"
	},
	"devDependencies": {
		"ava": "0.25.0",
		"xo": "^0.23.0"
	},
	"xo": {
		"space": false,
		"semicolon": false,
		"capitalized-comments": [
